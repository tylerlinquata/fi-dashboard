{
  "extends": ["next", "next/core-web-vitals", "eslint:recommended"],
  "globals": {
    // react will always be defined, even if not imported
    "React": "readonly"
  },
  "rules": {
    // allows you to prefix unused variables with _
    "no-unused-vars": [1, { "args": "after-used", "argsIgnorePattern": "^_" }]
  }
}
